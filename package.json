    "url": "https://github.com/xxidbr9/web-design-boilerplate/issues"
  },
  "homepage": "https://github.com/xxidbr9/web-design-boilerplate#readme",
  "dependencies": {
    "gsap": "^3.3.4",
    "node-sass": "^4.14.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-node": "0.0.1-security",
    "babel-preset-env": "^1.7.0",
    "live-server": "^1.2.1",
    "nodemon": "^2.0.4"
  }
