{
  "name": "web-design-boilerplate",
  "version": "1.2.5",
  "description": "This is simple boiler plate web design with scss ",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test ok\"",
    "watch": "node-sass --watch ./assets/scss/style.scss ./assets/css/style.css --output-style compressed",
    "build": "nodemon --exec babel-node ./assets/js/* ",
    "start": "live-server & npm run build & npm run watch"
  },
